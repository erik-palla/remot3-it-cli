{
  "name": "remot3-it-cli",
  "version": "0.0.1",
  "description": "Command Line Interface for controlling devices registered in Remot3.it service",
  "preferGlobal": true,
  "bin": {
    "remotecli": "dist/index.js"
  },
  "main": "dist/index.js",
  "scripts": {
    "test": "NODE_ENV=test && mocha $(find tests -name *.test.js) --compilers js:babel-register --require babel-polyfill",
    "test:w": "npm t -- --watch",
    "prebuild": "rimraf dist",
    "build": "babel --out-dir dist --ignore *.test.js src",
    "coverage": "istanbul cover _mocha -- ./tests --compilers js:babel-register "
  },
  "keywords": [
    "Remot3.it",
    "Weaved",
    "CLI"
  ],
  "files": [
    "dist",
    "readme.md"
  ],
  "author": "Erik Palla <hello@erikpalla.com> (http://erikpalla.com/)",
  "license": "MIT",
  "dependencies": {
    "copy-paste": "^1.3.0",
    "inquirer": "^3.2.2",
    "remot3-it-api": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.1",
    "eslint": "^4.5.0",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-plugin-import": "^2.7.0",
    "mocha": "^3.5.0",
    "rimraf": "^2.6.1",
    "sinon": "^4.0.2"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erik-palla/remot3-it-cli"
  },
  "homepage": "https://github.com/erik-palla/remot3-it-cli#readme"
}
